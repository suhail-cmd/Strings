#python
class Solution:
    def frequencySort(self, s: str) -> str:
        res = ""
        freq = {}
        for char in s:
            if char not in freq:
                freq[char] = 1
            else:
                freq[char] += 1
        freq=dict(sorted(freq.items(), key=lambda item: item[1], reverse=True))
        keys,values = [],[]
        for key,value in freq.items():
            keys.append(key)
            values.append(value)
        for i in range(len(keys)):
            res+=(keys[i]*values[i])
        return res
