#python
class Solution:
    def hasSameDigits(self, s: str) -> bool:
        while len(s) > 2:
            new_s = ""
            n = len(s)
            for i in range(n - 1):
                a = int(s[i])
                b = int(s[i + 1])
                new_s += str((a + b) % 10)
            s = new_s 
        if s[0] == s[1]:
            return True
        return False
            
